{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "web-ciab-site": {
      "i18n": {
        "sourceLocale": "pt",
        "locales": {
          "en": "src/locale/messages/messages.en.xlf"
        }
      },
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "sass"
        }
      },
      "root": "src",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/web-ciab-site",
            "index": "src/index.html",
            "rebaseRootRelativeCssUrls": true,
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "i18nLocale": "pt",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets/",
              "src/config.json",
              "src/web.config"
            ],
            "styles": [
              "src/custom-theme.scss",
              "src/styles.sass"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "assets": [
                "src/favicon.ico",
                {
                  "glob": "**/*",
                  "input": "src/assets/",
                  "output": "../assets/"
                },
                {
                  "glob": "config.json",
                  "input": "src/",
                  "output": "../"
                },
                {
                  "glob": "web.config",
                  "input": "src/",
                  "output": "../"
                }
              ]
            },
            "en": {
              "i18nLocale": "en",
              "localize": ["en"]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "web-ciab-site:build"
          },
          "configurations": {
            "test": {
              "browserTarget": "web-ciab-site:build:test"
            },
            "production": {
              "browserTarget": "web-ciab-site:build:production"
            },
            "en": {
              "browserTarget": "web-ciab-site:build:en"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-ciab-site:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.sass"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "web-ciab-site:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "web-ciab-site:serve:production"
            }
          }
        },
        "xliffmerge": {
          "builder": "@ngx-i18nsupport/tooling:xliffmerge",
          "options": {
            "xliffmergeOptions": {
              "i18nFormat": "xlf",
              "srcDir": "src/locale",
              "genDir": "src/locale/messages",
              "defaultLanguage": "pt",
              "languages": [
                "pt",
                "en"
              ],
              "beautifyOutput": true
            }
          }
        }
      }
    }
  },
  "defaultProject": "web-ciab-site"
}